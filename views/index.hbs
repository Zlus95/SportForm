  <script type="text/javascript">
    jQuery(document).ready(function () {

      $(document).on("click", ".item .neck input", function (e) {
        $colar = $(this).val();
        $item_id = $(document).find(".item.active .neck input[value='" + $colar + "']").data('id');
        // $(document).find(".item.active .neck input[data-id='" + $item_id + "']:checked").prop("checked", false);
        $(this).attr("checked");
        jQuery(".tab-pane.active .item.active .svg-img").hide().fadeOut("slow");
        $sleeve = $(document).find(".item.active .sleeve input:checked").val();
        getsvg();
      });
      $(document).on("click", ".item .sleeve input", function (e) {
        $sleeve = $(this).val();
        $item_id = $(document).find(".item.active .sleeve input[value='" + $sleeve + "']").data('id');
        // $(document).find(".item.active .sleeve input[data-id='" + $item_id + "']:checked").prop("checked", false);
        $(this).attr("checked");
        jQuery(".tab-pane.active .item.active .svg-img").hide().fadeOut("slow");
        $colar = $(document).find(".item.active .neck input:checked").val();

        getsvg();
      });
      function getsvg() {
        console.log($item_id + ' ' + $sleeve + ' ' + $colar);
        $.ajax({    //create an ajax request to display.php
          type: "GET",
          url: "/constructor/getsvg.php",
          data: { "data_id": $item_id, "sleeve": $sleeve, "colar": $colar },
          dataType: "html",   //expect html to be returned
          success: function (response) {
            $newsvg = $(response);
            $maincolor = $(".tab-pane.active .item.active .blockColor .selectedColor[data-style='fil0']").css('background-color');
            $d1color = $(".tab-pane.active .item.active .blockColor .selectedColor[data-style='color1']").css('background-color');
            $d2color = $(".tab-pane.active .item.active .blockColor .selectedColor[data-style='color2']").css('background-color');
            $d3color = $(".tab-pane.active .item.active .blockColor .selectedColor[data-style='color3']").css('background-color');
            $d4color = $(".tab-pane.active .item.active .blockColor .selectedColor[data-style='color4']").css('background-color');
            $colorLine = $(".tab-pane.active .item.active .blockColor .selectedColor[data-style='colorLine']").css('background-color');

            $($newsvg).find(".fil0").css("fill", $maincolor);
            $($newsvg).find(".color1").css("fill", $d1color);
            $($newsvg).find(".color2").css("fill", $d2color);
            $($newsvg).find(".color3").css("fill", $d3color);
            $($newsvg).find(".color4").css("fill", $d4color);
            $($newsvg).find(".colorLine").css("fill", $colorLine);

            $(".tab-pane.active .item.active .svg-img").html($newsvg).hide().fadeIn("slow");
          }
        });
        if ($sleeve === "long") {
          // console.log($sleeve);
          // $(".tab-pane.active .item.active .svg-img.basic").addClass($sleeve +' '+ $colar);
          $(".tab-pane.active .item.active .svg-img").attr("class", "svg-img ").addClass($colar + ' ' + $sleeve);
        } else {
          // console.log($sleeve);
          $(".tab-pane.active .item.active .svg-img").attr("class", "svg-img").addClass($colar + ' ' + $sleeve);
        }

      }

    });
  </script>
